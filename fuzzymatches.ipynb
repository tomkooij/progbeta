    "    return [(needle[:idx], needle[idx+1:]) for idx in range(len(needle))]\n",
    "\n",
    "split_needle(\"abcde\")\n",
    "assert(split_needle(\"abcde\") == [('', 'bcde'), ('a', 'cde'), ('ab', 'de'), ('abc', 'e'), ('abcd', '')])"
